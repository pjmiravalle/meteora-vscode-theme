  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#444B6A",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Comment DocBlock - @ Tag",
      "scope": ["storage.type.class", "storage.type.class punctuation"],
      "settings": {
        "foreground": "#7AA2F7"
      }
    },

    {
      "name": "Comment DocBlock - Link Description",
      "scope": ["constant.other.description"],
      "settings": {
        "foreground": "#787C99"
      }
    },

    {
      "name": "Comment DocBlock - Object",
      "scope": ["variable.other.description"],
      "settings": {
        "foreground": "#85D0B7"
      }
    },

    {
      "name": "Built-in, Constant, Number, Enum Member",
      "scope": [
        "constant",
        "entity.name.constant",
        "support.type.builtin",
        "variable.other.constant",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#ffac7c"
      }
    },

    {
      "name": "Entity, Type",
      "scope": [
        "entity",
        "entity.name.type",
        "entity.name.type.enum",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#73DACA"
      }
    },

    {
      "name": "Keyword",
      "scope": ["keyword.control", "keyword.other"],
      "settings": {
        "foreground": "#BB9AF7",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Keyword - Default",
      "scope": ["keyword.control.default"],
      "settings": {
        "fontStyle": "italic bold"
      }
    },

    {
      "name": "Method",
      "scope": [
        "entity.name.command",
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#f981d1"
      }
    },

    {
      "name": "Namespace",
      "scope": ["entity.name.namespace", "support.other.namespace"],
      "settings": {
        "foreground": "#7AA2F7"
      }
    },

    {
      "name": "Namespace - Operator",
      "scope": ["keyword.operator.namespace"],
      "settings": {
        "foreground": "#787C99"
      }
    },

    {
      "name": "Namespace - String",
      "scope": ["meta.import string"],
      "settings": {
        "foreground": "#85D0B7"
      }
    },

    {
      "name": "Operator",
      "scope": [
        "keyword.operator.access",
        "keyword.operator.arithmetic",
        "keyword.operator.arrow",
        "keyword.operator.assignment",
        "keyword.operator.bitwise",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.range",
        "keyword.operator.ternary",
        "keyword.operator.type"
      ],
      "settings": {
        "foreground": "#C0CAF5",
        "fontStyle": ""
      }
    },

    {
      "name": "Plain Text",
      "scope": ["meta.jsx.children", "text.html"],
      "settings": {
        "foreground": "#a6afd3"
      }
    },

    {
      "name": "Property",
      "scope": ["meta.attribute", "variable.other.property"],
      "settings": {
        "foreground": "#7AA2F7"
      }
    },

    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#A9B1D6"
      }
    },

    {
      "name": "Punctuation - Template Expression",
      "scope": ["punctuation.definition.template-expression"],
      "settings": {
        "foreground": "#906fce"
      }
    },

    {
      "name": "Special",
      "scope": [
        "keyword.operator.spread",
        "keyword.operator.borrow.and",
        "keyword.operator.dereference",
        "keyword.operator.expression.instanceof",
        "keyword.operator.expression.typeof",
        "meta.statement.command constant.other.option",
        "punctuation.definition.anchor",
        "punctuation.definition.alias",
        "source.shell keyword.operator.logical",
        "variable.language.this",
        "variable.language.this punctuation.definition.variable",
        "variable.language.special.self",
        "variable.parameter.function.language.special.self"
      ],
      "settings": {
        "foreground": "#ffac7c",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Storage",
      "scope": ["storage.modifier", "storage.type", "storage.type.class"],
      "settings": {
        "foreground": "#906fce",
        "fontStyle": "italic"
      }
    },

    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#929BBA"
      }
    },

    {
      "name": "String - Interpolation",
      "scope": ["meta.interpolation"],
      "settings": {
        "foreground": "#97BEF9"
      }
    },

    {
      "name": "String - Interpolation Punctuation",
      "scope": ["punctuation.definition.interpolation"],
      "settings": {
        "foreground": "#89ddff"
      }
    },

    {
      "name": "Variable, Property, Parameter",
      "scope": [
        "entity.name.label",
        "entity.name.type.anchor",
        "punctuation.definition.variable",
        "string.unquoted.argument",
        "variable",
        "variable.parameter",
        "variable.other.constant",
        "variable.other.object",
        "variable.language"
      ],
      "settings": {
        "foreground": "#97BEF9"
      }
    },

    {
      "name": "JSON, YAML - Key",
      "scope": ["support.type.property-name", "source.yaml entity.name.tag"],
      "settings": {
        "foreground": "#7AA2F7"
      }
    },

    {
      "name": "JSON, TOML, YAML - String",
      "scope": [
        "source.json string",
        "source.toml string",
        "source.yaml string"
      ],
      "settings": {
        "foreground": "#85D0B7"
      }
    },

    {
      "name": "Log - Date",
      "scope": ["log.date"],
      "settings": {
        "foreground": "#787C99"
      }
    },

    {
      "name": "Log - Info",
      "scope": ["log.info"],
      "settings": {
        "foreground": "#7AA2F7"
      }
    },

    {
      "name": "Markdown - Code Language",
      "scope": ["fenced_code.block.language.markdown"],
      "settings": {
        "foreground": "#f981d1",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Markdown - Code Inline",
      "scope": ["markup.inline.raw.string.markdown"],
      "settings": {
        "foreground": "#f981d1",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Markdown - Heading 1",
